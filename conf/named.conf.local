//include "/etc/bind/zones.rfc1918";

zone "kawauso-lab.example" {
	type master;
	file "/etc/bind/db.kawauso-lab.example";
	allow-query { any; };
	allow-update { none; };
};

zone "0.168.192.in-addr.arpa" {
	type master;
	file "/etc/bind/db.0.168.192.in-addr.arpa";
	allow-query { any; };
	allow-update { none; };
};
